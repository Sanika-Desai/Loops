#include<iostream>
using namespace std;

main()
{



{
    int i;

    for(i=1;i<=10;i++)
    {
        cout<<"hello world :"<<i<<endl;
    }
}


}
/*
OUTPUT
hello world :1
hello world :2
hello world :3
hello world :4
hello world :5
hello world :6
hello world :7
hello world :8
hello world :9
hello world :10
*/
